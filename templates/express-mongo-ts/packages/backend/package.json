{
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "prebuild": "del build/src/public/*; tsc --build --clean",
    "build": "tsc --build",
    "postbuild": "cpy src/public build/",
    "dev": "nodemon -e ts,js --exec ts-node --files ./src/index.ts",
    "test": "NODE_ENV=test jest --maxWorkers=4",
    "check": "npm run build && npm run test && npm run lint",
    "start": "ts-node ./build/src/index",
    "lint": "eslint src/**/*.ts test/**/*.ts"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "mongoose": "^6.4.4"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "cpy-cli": "^4.1.0",
    "del-cli": "^4.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^28.1.3",
    "mongodb-memory-server": "^8.7.2",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
